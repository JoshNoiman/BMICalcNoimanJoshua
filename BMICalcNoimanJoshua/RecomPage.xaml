<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BMICalcNoimanJoshua.RecomPage"
             Title="RecomPage"
             NavigationPage.HasNavigationBar="False">
    <VerticalStackLayout Spacing="50" Margin="20">
        <Frame HorizontalOptions="Center"
               Padding="50"
               HasShadow="True"
               BorderColor="AntiqueWhite">
            <VerticalStackLayout Spacing="10">
                <Label x:Name="LblRecom" HorizontalOptions="Center" FontSize="10"/>
                <Button Text="BMI Result Page" Clicked="Button_Clicked_2"/>
                <Button Text="Input Page" Clicked="Button_Clicked_3"/>
            </VerticalStackLayout>
        </Frame>
    </VerticalStackLayout>
</ContentPage>